menu "AI"

config USING_UAI_SDK
    bool
    default y

menu "Data"

    config UAI_USING_DATA_COLLECT
        bool "Enable Data Sample"
        select USING_UAI_SDK
        select UAI_USING_UTILS
        default n

endmenu

source "$OS_ROOT/thirdparty/matrixCpp/common/Kconfig"

source "$OS_ROOT/thirdparty/matrixCpp/feature/Kconfig"

source "$OS_ROOT/thirdparty/matrixCpp/library/Kconfig"

source "$OS_ROOT/thirdparty/matrixCpp/test/Kconfig"

endmenu
